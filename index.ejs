<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1, maximum=1, user-scalable=no">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>vue-element-placeholder-polyfill</title>
    <style>
        img {
          width: 100px;
        }
    </style>
    <link rel="stylesheet" href="https://unpkg.com/element-ui/lib/theme-chalk/index.css">
</head>
<body>
    <div id="app">
        <el-input
          class="account-input"
          v-model="userName"
          ref="userName"
          placeholder="请输入账号"
          @focus="inputFocus('userName', '请输入账号')"
          @blur="inputBlur('userName', '请输入账号')"
          v-inputie9:userName="{placeholder: '请输入账号'}">
        </el-input>
        <el-input
          class="account-input"
          v-model="password"
          ref="password"
          type="password"
          placeholder="请输入密码"
          @focus="inputFocus('password', '请输入密码')"
          @blur="inputBlur('password', '请输入密码')"
          v-inputie9:password="{placeholder: '请输入密码'}">
        </el-input>
      </div>
    <!-- <script src="./ie9-oninput-polyfill.js"></script> -->
    <script src="https://unpkg.com/vue@2.5.16/dist/vue.js"></script>
    <script src="https://unpkg.com/element-ui/lib/index.js"></script>
</body>
<script type="module">
    var main = {
      data() {
        return {
          userName: '',
          password: '',
        };
      },
      mixins: [
        inputie9Mixins.default,
      ],
    };
    var InputIE9 = Vue.extend(main);
    new InputIE9().$mount('#app');
  </script>
</html>